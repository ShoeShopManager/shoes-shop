<!-- Using layout -->

<p>Hello from the product index view!</p>

<p>Product list</p>

<ul>
    <% products.map(product => { %>
    <li>
        <a href="/product/<%= product.id %>">
            <span><%= product.name %></span>
            <span><%= product.price %></span>
        </a>
    </li>
    <% }) %>
</ul>

